<odoo>
    <data>                      

        <!-- Mutasi Veneer Klin Dry -->
        <record id="pwk_mutasi_veneer_klindry_tree" model="ir.ui.view">
            <field name="name">Mutasi Veneer Klin Dry</field>
            <field name="model">pwk.mutasi.veneer.klindry</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="date"/>
                    <field name="state"/>                    
                </tree>
            </field>
        </record>

        <record id="pwk_mutasi_veneer_klindry_form" model="ir.ui.view">
            <field name="name">Mutasi Veneer Klin Dry</field>
            <field name="model">pwk.mutasi.veneer.klindry</field>
            <field name="arch" type="xml">
                <form>
                    <header>
                        <field name="state" widget="statusbar" readonly="1"/>
                        <button name="button_approve" type="object" string="Approve" states="Draft"/>
                        <button name="button_print" type="object" string="Print"/>
                    </header>
                    <sheet>
                        <label for="name" class="oe_edit_only" />
                        <h1>
                            <field name="name" readonly="1"/>
                        </h1>
                        <group>
                            <field name="date" class="oe_inline" required="1"/>
                            <field name="user_id" class="oe_inline" readonly="1"/>
                        </group>
                        <group>
                            <notebook>
                                <page string="Veneer Klin Dry">
                                    <button name="button_reload" string="Reload" 
                                        type="object" class="oe_highlight"/>
                                    <group>
                                        <field name="line_ids" nolabel="1">
                                            <tree>
                                                <field name="product_id"/>
                                                <field name="tebal"/>
                                                <field name="lebar"/>
                                                <field name="panjang"/>
                                                <field name="grade"/>
                                                <field name="stock_awal_pcs" sum="Total"/>
                                                <field name="stock_masuk_pcs" sum="Total"/>
                                                <field name="stock_keluar_pcs" sum="Total"/>
                                                <field name="stock_akhir_pcs" sum="Total"/>
                                            </tree>
                                            <form>
                                                <group>
                                                    <field name="product_id"/>
                                                    <field name="tebal"/>
                                                    <field name="lebar"/>
                                                    <field name="panjang"/>
                                                    <field name="grade"/>
                                                </group>                                        
                                                <group>
                                                    <group string="Perhitungan Stock (Pcs)">
                                                        <field name="stock_awal_pcs"/>                  
                                                        <field name="stock_masuk_pcs"/> 
                                                        <field name="stock_keluar_pcs"/>
                                                        <field name="stock_akhir_pcs"/>
                                                    </group>
                                                    <group string="Perhitungan Stock (Vol)">
                                                        <field name="stock_awal_vol"/>
                                                        <field name="stock_masuk_vol"/>
                                                        <field name="stock_keluar_vol"/>
                                                        <field name="stock_akhir_vol"/>
                                                    </group>
                                                </group>
                                                <group>
                                                    <group string="Akumulasi Stock (Pcs)">
                                                        <field name="acc_stock_masuk_pcs"/> 
                                                        <field name="acc_stock_keluar_pcs"/> 
                                                    </group>
                                                    <group string="Akumulasi Stock (Vol)">
                                                        <field name="acc_stock_masuk_vol"/>      
                                                        <field name="acc_stock_keluar_vol"/>
                                                    </group>
                                                </group>
                                            </form>
                                        </field>
                                    </group>
                                </page>                                
                                <page string="Veneer Re - Klin Dry">
                                    <button name="button_reload_re" string="Reload" 
                                        type="object" class="oe_highlight"/>
                                    <group>
                                        <field name="reline_ids" nolabel="1">
                                            <tree>
                                                <field name="product_id"/>
                                                <field name="tebal"/>
                                                <field name="lebar"/>
                                                <field name="panjang"/>
                                                <field name="grade"/>
                                                <field name="stock_awal_pcs" sum="Total"/>
                                                <field name="stock_masuk_pcs" sum="Total"/>
                                                <field name="stock_keluar_pcs" sum="Total"/>
                                                <field name="stock_akhir_pcs" sum="Total"/>
                                            </tree>
                                            <form>
                                                <group>
                                                    <field name="product_id"/>
                                                    <field name="tebal"/>
                                                    <field name="lebar"/>
                                                    <field name="panjang"/>
                                                    <field name="grade"/>
                                                </group>                                        
                                                <group>
                                                    <group string="Perhitungan Stock (Pcs)">
                                                        <field name="stock_awal_pcs"/>                  
                                                        <field name="stock_masuk_pcs"/> 
                                                        <field name="stock_keluar_pcs"/>
                                                        <field name="stock_akhir_pcs"/>
                                                    </group>
                                                    <group string="Perhitungan Stock (Vol)">
                                                        <field name="stock_awal_vol"/>
                                                        <field name="stock_masuk_vol"/>
                                                        <field name="stock_keluar_vol"/>
                                                        <field name="stock_akhir_vol"/>
                                                    </group>
                                                </group>
                                                <group>
                                                    <group string="Akumulasi Stock (Pcs)">
                                                        <field name="acc_stock_masuk_pcs"/> 
                                                        <field name="acc_stock_keluar_pcs"/> 
                                                    </group>
                                                    <group string="Akumulasi Stock (Vol)">
                                                        <field name="acc_stock_masuk_vol"/>      
                                                        <field name="acc_stock_keluar_vol"/>
                                                    </group>
                                                </group>
                                            </form>
                                        </field>
                                    </group>
                                </page>
                            </notebook>
                        </group>
                    </sheet>
                    <div class="oe_chatter">
                        <field name="message_follower_ids" widget="mail_followers"/>
                        <field name="activity_ids" widget="mail_activity"/>
                        <field name="message_ids" widget="mail_thread"/>
                    </div>
                </form>
            </field>
        </record>

    </data>
</odoo>
